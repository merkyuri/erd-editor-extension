{
  "name": "erd-editor-schema-helper",
  "displayName": "ERD Editor-Schema Helper",
  "description": "Entity Relationship Diagram Editor.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:erd-editor-schema-helper.webview",
    "onWebviewPanel:erd-editor-schema-helper.webview",
	  "onCustomEditor:erd-editor-schema-helper.editor"
  ],
  "main": "src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "erd-editor-schema-helper.webview",
        "title": "ERD Editor-Schema Helper"
      }
    ],
    "customEditors": [
      {
        "viewType": "erd-editor-schema-helper.editor",
        "displayName": "ERD Editor-Schema Helper",
        "selector": [
          {
            "filenamePattern": "*.erdsh"
          }
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "2.5.1"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "lint-staged": {
    "*.{js, jsx, html, css, json}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  }
}
